{
  "server": "hub.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",

  "capabilities": {
    "bstack:options": {
      "buildName": "browserstack-build-1",
      "sessionName": "BStack Parallel Test",
      "browserVersion": "latest",
      "debug": true,
      "source": "cucumber:sample-master-v1.0"
    }
  },

  "environments": [
    {
      "browserName": "chrome"
    },
    {
      "browserName": "firefox"
    },
    {
      "bstack:options": {
        "os": "OS X"
      },
      "browserName": "safari"
    }
  ]
}
