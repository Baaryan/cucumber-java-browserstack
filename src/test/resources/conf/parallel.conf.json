{
  "server": "hub.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",

  "capabilities": {
    "build": "cucumber-java-browserstack",
    "browserstack.debug": true,
    "browserstack.source": "cucumber:sample-selenium-3-v1.0"
  },

  "environments": [
    {
      "browser": "chrome"
    },
    {
      "browser": "firefox"
    },
    {
      "browser": "safari"
    }
  ]
}
